{
  "name": "@poulpi/domtojson",
  "version": "0.1.22",
  "description": "simple domtojson",
  "main": "dist/index.umd.js",
  "module": "dist/index.mjs.js",
  "directories": {
    "test": "test"
  },
  "types": "types/",
  "scripts": {
    "clean": "rm -r dist && rm -r types",
    "start": "parcel test/test.html",
    "build-prod": "parcel build src/index.ts --global DTM && parcel build src/index.ts -t node -o index.umd.js && parcel build src/index.ts --bundle-node-modules -o index.mjs.js && tsc src/index.ts --outDir types/ -d --emitDeclarationOnly"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Poulpinounette/domtojson.git"
  },
  "keywords": [
    "domparser",
    "json",
    "dom2json",
    "json2dom"
  ],
  "author": "poulpi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Poulpinounette/domtojson/issues"
  },
  "homepage": "https://github.com/Poulpinounette/domtojson#readme",
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "typescript": "^3.6.4"
  }
}
