    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "es6",
        "moduleResolution": "Node",
        "target": "ESNext",
        "allowJs": true,
        "strictNullChecks": false
    },
    "include": [
        "./src/**/*"
    ]
}